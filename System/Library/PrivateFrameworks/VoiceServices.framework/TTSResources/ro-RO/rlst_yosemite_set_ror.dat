/([a-zA-Z])\.int\b/ --> "$1 punct int"
/([a-zA-Z])\.co\b/ --> "$1 punct C O"
/([a-zA-Z])\.es\b/ --> "$1 punct E S"
/([a-zA-Z])\.de\b/ --> "$1 punct D E"
