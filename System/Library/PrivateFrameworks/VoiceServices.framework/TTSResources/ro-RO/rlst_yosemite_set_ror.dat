[data]
# rule for "dot com" bug  (radar 7768195 Bugzilla16288)
/([a-zA-Z])\.org\b/ --> "$1 punct org"
/([a-zA-Z])\.net\b/ --> "$1 punct net"
/([a-zA-Z])\.gov\b/ --> "$1 punct gov"
/([a-zA-Z])\.edu\b/ --> "$1 punct edu"
/([a-zA-Z])\.info\b/ --> "$1 punct info"
/([a-zA-Z])\.biz\b/ --> "$1 punct biz"
