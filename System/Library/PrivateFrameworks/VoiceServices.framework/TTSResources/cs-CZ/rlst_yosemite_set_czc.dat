[data]
#Put meters and kilometers in Acc / Gen case after preposition "po"
/([Pp][Oo] [0-9]+ )km[.]? /i   -->   "$1kilometrů "
/([Pp][Oo] [0-9]+ )m[.]? /i   -->   "$1metrů "
