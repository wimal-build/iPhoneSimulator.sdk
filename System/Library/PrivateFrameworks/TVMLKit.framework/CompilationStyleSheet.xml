<?xml version="1.0" encoding="UTF-8"?>
<document>
    <head>
        <style>
            .list {
                tv-align:left;
                width: 924;
				padding: 50 78 90 0;
                margin:0 0 0 160;
            }
            .listHeader {
                margin: 66 0 15;
                padding: 0 20;
            }
            .listHeaderTitle {
                tv-text-style: title2;
                text-align: left;
                tv-text-max-lines: 2;
                color: rgb(0, 0, 0);
            }
            .listHeaderSubtitle {
                text-align: left;
                color: rgb(0, 0, 0);
            }
            .listHeaderText {
                tv-align: left;
                text-align: left;
                color: rgb(0, 0, 0);
            }
            .lilOrdinal {
                tv-text-style: subtitle3;
                margin:13 0 0 9;
            }
            .lilTitle {
                tv-text-style: subtitle3;
            }
            .lilSubtitle {
                tv-text-style: caption1;
            }
            .lilDecorationLabel {
                tv-text-style: body;
            }
            .itemBanner {
                margin: 136 90 0 105;
            }

            .collection {
                tv-interitem-spacing: 44;
                padding: 0 20;
                margin: 64 0 0;
            }
            .collectionHeader {
                margin: 0 0 23;
            }
            .collectionHeaderTitle {
                tv-text-style: body;
                tv-text-max-lines: 1;
                text-align: left;
                tv-position: bottom-left;
                color: rgb(0, 0, 0);
            }
            .collectionLockupTitle {
                tv-text-style: caption2;
                tv-text-max-lines: 1;
                margin: 4px 0px 0px;
                text-align: center;
                tv-text-highlight-style: marquee-on-highlight;
                color:rgba(0, 0, 0, 0.6);
                tv-highlight-color: rgb(255.0, 255.0, 255.0);
            }
            .collectionLockupSubtitle {
                tv-text-style: caption2;
                tv-text-max-lines: 1;
                margin: 0;
                text-align: center;
                color:rgba(0, 0, 0, 0.4);
                tv-highlight-color: rgb(255.0, 255.0, 255.0);
            }
            .monogram {
                border-radius:circle;
            }
            .itemBannerRow {
                tv-content-align: top;
            }

            @media tv-template and (-tv-uber) {
                .list {
                    width: 1004;
                    padding: 43 78 43 0;
                }
                .listHeader {
                    margin: -24 0 15;
                }
                .heroImg {
                    width: 556;
                    height: 556;
                }
                .itemBanner {
                    margin: -24 90 0 110;
                    width: 556;
                }
                .itemBannerRow {
                    margin: 22 0 0;
                    tv-interitem-spacing: 23;
                }
                .itemBannerButtonLockup {
                    width: 122;
                }
                .shelf {
                    tv-interitem-spacing: 98;
                }
            }

            @media tv-template and (-tv-bg-uber) {
                .itemBanner {
                    margin: 836 90 0 105;
                }
                .itemBannerRow {
                    margin: 0;
                }
            }

            @media tv-template and (tv-theme:dark) {
                .listHeaderTitle {
                    color: rgb(255, 255, 255);
                }
                .listHeaderSubtitle {
                    color: rgb(255, 255, 255);
                }
                .listHeaderText {
                    color: rgb(255, 255, 255);
                }
                .collectionHeaderTitle {
                    color: rgb(255, 255, 255);
                }
                .collectionLockupTitle {
                    color:rgba(255, 255, 255, 0.5);
                }
                .collectionLockupSubtitle {
                    color:rgba(255, 255, 255, 0.3);
                }
            }
        </style>
    </head>
    
    <compilationTemplate>
        <list class="list">
			<relatedContent>
				<itemBanner class="itemBanner">
					<img class="heroImg" />
					<heroImg class="heroImg" />
					<row class="itemBannerRow">
						<buttonLockup class="baseButtonLockup itemBannerButtonLockup">
						</buttonLockup>
					</row>
				</itemBanner>
			</relatedContent>
            <header class="listHeader">
                <title class="listHeaderTitle"/>
                <subtitle class="listHeaderSubtitle"/>
                <text class="listHeaderText" />
                <row>
                    <text class="listHeaderText" />
                </row>
            </header>

            <section>
                <listItemLockup>
                    <title class="lilTitle"/>
                    <subtitle class="lilSubtitle" />
                    <decorationLabel class="lilDecorationLabel" />
                </listItemLockup>

                <shelf class="collection">
                    <header class="collectionHeader">
                        <title class="collectionHeaderTitle"/>
                    </header>
                    <section>
                        <lockup>
                            <title class="collectionLockupTitle" />
                            <subtitle class="collectionLockupSubtitle"/>
                        </lockup>
                        <monogramLockup>
                            <monogram class="monogram" />
                            <title class="collectionLockupTitle" />
                            <subtitle class="collectionLockupSubtitle"/>
                        </monogramLockup>
                    </section>
                </shelf>
                
                <grid class="collection">
                    <header class="collectionHeader">
                        <title class="collectionHeaderTitle"/>
                    </header>
                    <section>
                        <lockup>
                            <title class="collectionLockupTitle" />
                            <subtitle class="collectionLockupSubtitle"/>
                        </lockup>
                        <monogramLockup>
                            <monogram class="monogram" />
                            <title class="collectionLockupTitle" />
                            <subtitle class="collectionLockupSubtitle"/>
                        </monogramLockup>
                    </section>
                </grid>
            </section>
        </list>
    </compilationTemplate>
</document>
