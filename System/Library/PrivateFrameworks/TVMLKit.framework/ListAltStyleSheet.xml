<?xml version="1.0" encoding="UTF-8"?>
<document>
    <head>
        <style>
            .list {
                tv-align: left;
                width: 567;
				padding: 50 81 60 0;
                margin:0 0 0 115;
                tv-interitem-spacing: 16;
            }
            .rcLockup {
                margin: 136 238 0 149;
            }

            .rcActivity {
                margin: 0 90 0 0;
            }
            
            .rcGrid {
                padding: 0 90 60 0;
                tv-interitem-spacing: 51px;
            }
            .rcGridHeader {
                padding: 0px 0px 40px;
            }
            .rcGridHeaderTitle {
                tv-text-style: headline;
                color: rgb(0, 0, 0);
                text-align: left;
                tv-position: center;
                tv-align: left;
            }
            .rcGridHeaderDesc {
                tv-text-style: body;
                color: rgb(0, 0, 0);
                text-align: left;
                tv-position: center;
                tv-align: left;
                tv-text-max-lines: 4;
            }
            .rcGridLockup {
            }
            .rcGridLockupImg {
                margin: 0 0;
            }
            .rcGridLockupTitle {
                tv-text-style: body;
                tv-text-max-lines:1;
                margin: 4px -24px 0px;
                text-align: center;
                color:rgba(0, 0, 0, 0.6);
                tv-highlight-color:rgb(255, 255, 255);
                tv-text-highlight-style: show-on-highlight;
            }
            .rcGridLockupSubtitle {
                tv-text-style: footnote;
                tv-text-max-lines:1;
                margin: 0 -24 0;
                text-align: center;
                color: rgba(0, 0, 0, 0.4);
                tv-highlight-color:rgb(255, 255, 255);
                tv-text-highlight-style: show-on-highlight;
            }

            @media tv-template and (tv-theme:dark) {
                .rcGridHeaderTitle {
                    color: rgb(255, 255, 255);
                }
                .rcGridHeaderDesc {
                    color: rgb(255, 255, 255);
                }
                .rcGridLockupTitle {
                    color:rgba(0, 0, 0, 0.5);
                }
                .rcGridLockupSubtitle {
                    color:rgba(0, 0, 0, 0.3);
                }
            }

            @media tv-template and (-tv-uber) {
                .rcLockup {
                    margin: -100 238 0 149;
                }
            }

            @media tv-template and (-tv-uber) and (-tv-header) {
                .list {
					width: 566;
					padding: -43 80 43 0;
                    margin: -120 0 0 120;
                }
				.rcLockup {
					margin: -100 238 0 150;
				}
				.rcGrid {
					margin: 0 0 0 1;
				}
            }
        </style>
    </head>
    
    <listAltTemplate>
        <relatedContent>
            <lockup class="rcLockup">
                <img class="rcImg"/>
                <title class="rcLockupTitle" />
                <subtitle class="rcLockupSubtitle" />
                <description class="rcDescription" />
            </lockup>
            <grid class="rcGrid">
                <header class="rcGridHeader">
                    <title class="rcGridHeaderTitle"></title>
                    <description class="rcGridHeaderDesc"></description>
                </header>
                
                <section>
                    <lockup class="rcGridLockup">
                        <img class="rcGridLockupImg" />
                        <title class="rcGridLockupTitle"></title>
                        <subtitle class="rcGridLockupSubtitle"></subtitle>
                    </lockup>
                </section>
            </grid>
            <activityIndicator class="rcActivity">
            </activityIndicator>
        </relatedContent>

        <list class="list" />
    </listAltTemplate>
</document>
