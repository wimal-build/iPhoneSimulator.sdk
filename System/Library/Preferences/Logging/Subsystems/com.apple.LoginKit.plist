<plist version="1.0">
    <dict>
        <key>DEFAULT-OPTIONS</key>                  <!-- default log level and persistence behavior for all categories in the subsystem -->
        <dict>
            <key>Level</key>
            <dict>
                <key>Enable</key>
                <string>Inherit</string>
                <key>Persist</key>
                <string>Inherit</string>
            </dict>
            <key>TTL</key>                          <!-- Time To Live (in days) -->
            <dict>
                <key>Default</key>
                <integer>10</integer>
                <key>Info</key>
                <integer>0</integer>
                <key>Debug</key>
                <string>0</string>
            </dict>
            <key>Event-Log</key>
            <dict>
                <key>Enabled</key>
                <string>Inherit</string>
            </dict>
        </dict>
    </dict>
</plist>
